{
    "name": "laragis/core",
    "description": "The Laragis Platform based on Laravel Framework.",
    "keywords": [
        "laravel",
        "admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Truong Thanh Tung",
            "email": "ttungbmt@gmail.com"
        }
    ],
    "require": {
        "laravel/nova": "*",
        "laragis/form": "*@dev",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.1",
        "beyondcode/nova-tinker-tool": "^1.1",
        "bolechen/nova-activitylog": "^0.2.1",
        "cybercog/laravel-nova-ban": "^1.1"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "Laragis\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\Providers\\NovaServiceProvider",
                "Laragis\\Providers\\LaragisServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
